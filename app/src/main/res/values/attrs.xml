<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LimitScrollSeekBar">
        <attr name="limit_max_progress" format="integer"/>
        <attr name="limit_min_progress" format="integer"/>
        <attr name="isShowBubble" format="boolean"/>
        <attr name="bubbleImg" format="reference"/>
        <attr name="arrowImageSrc" format="reference"/>
        <attr name="titleTextSize" format="dimension"/>
        <attr name="titleTextColor" format="color"/>
        <attr name="lineHeight" format="dimension"/>
        <attr name="bubbleMarginBottom" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="BubbleView">
        <attr name="bubbleColor" format="color"/>
        <attr name="bubbleTextColor" format="color"/>
        <attr name="bubbleTextSize" format="dimension"/>
        <attr name="bubbleWidth" format="dimension"/>
        <attr name="bubbleHeight" format="dimension"/>
        <attr name="bubbleCornerRadius" format="dimension"/>
    </declare-styleable>
</resources>